+
+        int[][] matrix = new int[m][n];
+        for (int i = 0; i < m; i++) {
+            for (int j = 0; j < n; j++) {
+                matrix[i][j] = new Random().nextInt(9);
+            }
+        }
+
+        int[][] trmatrix = new int[n][m];
