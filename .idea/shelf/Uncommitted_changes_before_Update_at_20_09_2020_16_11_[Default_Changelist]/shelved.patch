+                trmatrix[i][j] = matrix[j][i];
+            }
+        }
+
+        System.out.println("Начальная матрица");
+        for (int i = 0; i < m; i++) {
+            for (int j = 0; j < n; j++) {
+                System.out.print(matrix[i][j]);}
+            System.out.println();
+        }
+
+        System.out.println("После транспонирования");
+            for (int i = 0; i < n; i++) {
